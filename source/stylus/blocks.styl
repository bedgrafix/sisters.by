.im-popups
	.im-popup-inside
		&.all-window
			height 100%
			width 100%
			max-width 1878px
			max-height 100%
			padding-top 60px
			padding-bottom 60px
			@media $min_md
				padding-left 16px
				padding-right 16px
			.b-popup
				width 100%
				height 100%
	&.no-padding
		.im-popup
			padding-top 0
			padding-bottom 0

.fancybox-infobar, .fancybox-controls
	height 100%
	.fancybox-button--left
		position absolute
		left 20px
		top 50%
		border-radius 0 !important
	.fancybox-button--right
		position absolute
		right 20px
		top 50%
		border-radius 0 !important
// .slick-active.js-act
.slick-slide.js-act
	    border 2px solid #00519e
#vk_groups
	width 100% !important
	iframe
		width 100% !important
		
.icon-users
	width 20px
	height 15px

.b-gallery
	font-size 0px
 	margin 0px -10px
.b-gallery_item
	display inline-block
	width 20%
	vertical-align top
	box-sizing border-box
	padding 0px 10px
	margin-bottom 20px
	@media $max_md
		width 25%
	@media $max_xs
		width 50%
.b-gallery_img
	max-width 100%

.b-textarea + .b-popup__error
	margin-top -25px
